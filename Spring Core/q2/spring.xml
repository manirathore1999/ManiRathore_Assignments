<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="com.example.a1q2.QuestionList" name="QuestionList">

		<property name="questionId">
			<value>01</value>
		</property>

		<property name="question">
			<value>What is your name</value>
		</property>

		<property name="answers">
			<list>
				<value>Mani</value>
				<value>Rathore</value>
			</list>
		</property>
	</bean>

	<bean class="com.example.a1q2.QuestionMap" name="QuestionMap">

		<property name="questionId">
			<value>01</value>
		</property>

		<property name="question">
			<value>What is your name</value>
		</property>

		<property name="answers">
			<map>
				<entry key="01" value="Mani"></entry>
				<entry key="02" value="Rathore"></entry>
			</map>

		</property>
	</bean>

	<bean class="com.example.a1q2.QuestionSet" name="QuestionSet">

		<property name="questionId">
			<value>01</value>
		</property>

		<property name="question">
			<value>What is your name</value>
		</property>

		<property name="answers">
			<set>
				<value>Mani</value>
				<value>Rathore</value>
			</set>
		</property>
	</bean>




</beans>